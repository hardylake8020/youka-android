<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="@android:color/white">
    <include layout="@layout/default_head"/>
    <LinearLayout
            android:id="@+id/call_service_tel"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="20dp"
            android:paddingBottom="20dp"
            android:paddingLeft="20dp"
            android:background="@drawable/selector_lucency"
            android:orientation="horizontal">
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/service_tel_icon"
                android:drawablePadding="10dp"
                android:text="@string/view_tv_service_tel"
                android:textColor="@color/dark_black"
                android:textSize="16sp"/>
        <TextView
                android:id="@+id/service_tel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="20dp"
                android:gravity="right"
                android:textColor="@color/middle_gray"
                android:text="@string/view_tv_service_phone_number"
                android:textSize="16sp"/>
    </LinearLayout>
    <ImageView android:layout_width="match_parent" android:layout_height="1px"
               android:background="@color/bg_gray"
               android:layout_marginLeft="20dp"/>
    <RelativeLayout android:layout_width="match_parent" android:layout_height="wrap_content"
                    android:id="@+id/guidebook"
                    android:paddingTop="20dp"
                    android:paddingBottom="20dp"
                    android:paddingLeft="20dp"
                    android:background="@drawable/item_click_selector">
        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                  android:text="@string/view_tv_guidebook"
                  android:drawableLeft="@drawable/guidebook_icon"
                  android:drawablePadding="10dp"
                  android:textSize="16sp"
                  android:layout_alignParentLeft="true"
                  android:layout_centerVertical="true"
                  android:textColor="@color/dark_black"/>
        <ImageView android:layout_width="wrap_content" android:layout_height="wrap_content"
                  android:layout_alignParentRight="true"
                  android:layout_marginRight="10dp"
                  android:background="@drawable/icon_right_arrows"
                  android:layout_centerVertical="true"/>
    </RelativeLayout>
    <ImageView android:layout_width="match_parent" android:layout_height="1px"
               android:background="@color/bg_gray"
               android:layout_marginLeft="20dp"/>
    <RelativeLayout android:layout_width="match_parent" android:layout_height="match_parent"
            android:id="@+id/logEntry"></RelativeLayout>
</LinearLayout>